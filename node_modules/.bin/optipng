#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/optipng-bin@7.0.1/node_modules/optipng-bin/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/optipng-bin@7.0.1/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules:/home/jorgehara/Aptugo/builds/node_modules:/home/jorgehara/Aptugo/node_modules:/home/jorgehara/node_modules:/home/node_modules:/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/optipng-bin/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/optipng-bin@7.0.1/node_modules/optipng-bin/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/optipng-bin@7.0.1/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules:/home/jorgehara/Aptugo/builds/node_modules:/home/jorgehara/Aptugo/node_modules:/home/jorgehara/node_modules:/home/node_modules:/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/optipng-bin/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../optipng-bin/cli.js" "$@"
else
  exec node  "$basedir/../optipng-bin/cli.js" "$@"
fi
