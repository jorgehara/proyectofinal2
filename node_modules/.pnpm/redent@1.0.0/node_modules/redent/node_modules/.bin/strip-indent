#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules:/home/jorgehara/Aptugo/builds/node_modules:/home/jorgehara/Aptugo/node_modules:/home/jorgehara/node_modules:/home/node_modules:/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/redent@1.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules:/home/jorgehara/Aptugo/builds/node_modules:/home/jorgehara/Aptugo/node_modules:/home/jorgehara/node_modules:/home/node_modules:/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/home/jorgehara/Aptugo/builds/proyectofinal2/node_modules/.pnpm/redent@1.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
else
  exec node  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
fi
